{
  "name": "@rcmedeiros/template",
  "version": "0.9.2-dev",
  "description": "Template Project",
  "main": "index",
  "author": "Rafael Medeiros <rafael@rafaelmedeiros.eti.br>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/rcmedeiros/tslint-config/issues"
  },
  "scripts": {
    "build-ts": "tsc -p tsconfig.json & cp \"package.json\" \"dist/\"",
    "watch-ts": "tsc -d -w",
    "tslint": "tslint --project tsconfig.json --config src/tslint.json",
    "test": "nyc mocha -r ts-node/register test/*-test.ts",
    "coverage": "nyc --reporter=html mocha -r ts-node/register test/**/*-test.ts"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        75,
        90
      ],
      "statements": [
        80,
        95
      ]
    },
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "test/*"
    ],
    "cache": false,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcmedeiros/template.git"
  },
  "homepage": "https://github.com/rcmedeiros/tslint-config#readme",
  "devDependencies": {
    "@rcmedeiros/tslint-config": "^1.0.2",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/figlet": "^1.2.0",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "figlet": "^1.2.1"
  }
}